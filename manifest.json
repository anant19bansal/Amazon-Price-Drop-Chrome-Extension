{
    "manifest_version":2,
    "name": "Amazon Price Drop Extension",
    "version": "1.0",
    "description": "Getting started with amazon extension",
    "content_scripts":[
        {
            "matches":["https://www.amazon.in/gp/registry/wishlist?requiresSignIn=1&ref_=nav_AccountFlyout_wl"],
            "js":["content.js", "jquery-3.6.0.js"]
        }
    ],
    "background":{
        "scripts":["jquery-3.6.0.js", "background.js"]
    },
    "browser_action":{
        "default_popup": "popup.html"
    },
    "permissions":[
        "tabs"
    ]
}